within Example2.CustomComponents.PSSChangeParam.Test;
model TestPSS5Substructures
  extends Modelica.Icons.Example;
  PSS5Substructures pSS5Units(
    t6=150,
    pss1_Kw=0.01,
    pss1_Tw=1,
    pss2_Kw=0.05,
    pss2_Tw=10,
    pss3_Kw=0.1,
    pss3_Tw=20,
    pss4_Kw=0.5,
    pss4_Tw=40) annotation (Placement(transformation(extent={{-18,-20},{22,20}})));
  Modelica.Blocks.Sources.Sine sine(amplitude=0.01, f=0.5) annotation (Placement(transformation(extent={{-66,-10},{-46,10}})));
  Modelica.Blocks.Interfaces.RealOutput vs "PSS4Substructures Output"
    annotation (Placement(transformation(extent={{30,-10},{50,10}})));
equation
  connect(sine.y,pSS5Units. vSI) annotation (Line(points={{-45,0},{-22,0}}, color={0,0,127}));
  connect(pSS5Units.vs, vs)
    annotation (Line(points={{24,0},{40,0}}, color={0,0,127}));
  annotation (experiment(
      StopTime=150), preferredView = "diagram");
end TestPSS5Substructures;
